<view class="container">
  <image bind:tap="onGoToDetail" class="poster" src="{{movie.images.large}}"></image>
  <text class="title">{{movie.title}}</text>
  <view class="rate-container">
    <l-rate disabled="{{true}}" score="{{movie.rating.stars/10}}" size="20" count="5"/>
    <text class="score">{{movie.rating.average}}</text>
  </view>
</view>